					"function validateJsonResponse(requiredFields) {",
					"    pm.test(\"Response has valid JSON\", function () {",
					"        pm.response.to.be.json;",
					"    });",
					"",
					"    pm.test(\"Status code is 2xx or 4xx/5xx handled\", function () {",
					"        pm.expect(pm.response.code).to.be.oneOf([200, 201, 202, 204, 400, 401, 403, 404, 422, 429, 500]);",
					"    });",
					"",
					"    if (pm.response.code >= 200 && pm.response.code < 300) {",
					"        let jsonData = {};",
					"        try {",
					"            jsonData = pm.response.json();",
					"        } catch (e) {",
					"            pm.test(\"Response body is valid JSON\", () => pm.expect.fail(\"Response is not valid JSON\"));",
					"            return;",
					"        }",
					"",
					"        pm.test(\"Response contains required fields\", function () {",
					"            requiredFields.forEach(field => {",
					"                pm.expect(jsonData).to.have.property(field);",
					"            });",
					"        });",
					"    } else {",
					"        pm.test(\"Error response contains error key\", function () {",
					"            let jsonData = {};",
					"            try {",
					"                jsonData = pm.response.json();",
					"            } catch (e) {",
					"                pm.test(\"Error response is valid JSON\", () => pm.expect.fail(\"Error response is not valid JSON\"));",
					"                return;",
					"            }",
					"            pm.expect(jsonData).to.have.property('error');",
					"        });",
					"    }",
					"}",
					"",
					"// Switch case for each request to validate specific fields",
					"switch(pm.info.requestName) {",
					"    case \"Get All Instructors\":",
